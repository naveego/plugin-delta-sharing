{
  "id": "plugin-delta-sharing",
  "displayName": "Delta Sharing",
  "description": "Publisher which connects to Delta Sharing",
  "apiVersion": "1",
  "versionDescription": "Version 1",
  "version": "1.0.0",
  "iconFile": "icon.png",
  "executable": "PluginDeltaSharing",
  "kind": "publisher",
  "os": "$GOOS",
  "arch": "$GOARCH",
  "accessGroups": [],
  "canConfigure": true,
  "canProduceMultipleSchemas": true,
  "canAcceptQueryBasedSchemas": false,
  "canUseOAuth": false,
  "canConfigureWrite": false,
  "canWrite": false,
  "canConfigureReplication": false,
  "canWriteReplication": false,
  "querySchema": {
    "ui": {
      "ui:widget": "code",
      "ui:options": {
        "language": "sql"
      }
    },
    "schema": {
      "type": "string"
    }
  },
  "configSchema": {
    "ui": {
      "ui:order": [
        "bearer_token",
        "endpoint",
        "share_credentials_version"
      ],
      "bearer_token": {
        "ui:widget": "password"
      }
    },
    "schema": {
      "type": "object",
      "properties": {
        "bearer_token": {
          "type": "string",
          "title": "Bearer Token"
        },
        "endpoint": {
          "type": "string",
          "title": "Endpoint"
        },
        "share_credentials_version": {
          "type": "integer",
          "title": "Share Credentials Version"
        }
      },
      "required": [
        "bearer_token",
        "endpoint",
        "share_credentials_version"
      ]
    }
  }
}